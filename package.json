{
  "name": "multi-tenant-event-bookings",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "next build",
    "build:safe": "pnpm run generate:types && npx tsc --noEmit && next build",
    "build:vercel": "pnpm run generate:types && next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed": "ts-node --esm src/seed.ts",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.53.0",
    "@payloadcms/next": "3.53.0",
    "@payloadcms/payload-cloud": "3.53.0",
    "@payloadcms/richtext-lexical": "3.53.0",
    "@payloadcms/ui": "3.53.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "jose": "^6.0.13",
    "next": "15.4.4",
    "nodemon": "^3.1.10",
    "payload": "3.53.0",
    "react": "19.1.1",
    "sharp": "0.34.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
